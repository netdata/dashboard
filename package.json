    "@types/bootstrap": "^3",
    "@types/classnames": "^2.2.9",
    "@types/d3": "^4.11.0",
    "@types/dygraphs": "^1.1.8",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/jquery": "^3.3.30",
    "@types/node": "12.7.2",
    "@types/ramda": "^0.26.21",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "canvas": "^2.6.0",
    "cpx": "^1.5.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-react-hooks": "^2.0.1",
    "install-peers-cli": "^2.1.1",
    "jest-environment-jsdom-fourteen": "^0.1.0"
  },
  "scripts": {
    "start": "REACT_APP_SHOULD_USE_DEFAULT_CONTEXT=true react-scripts start",
    "start:console": "cp ../console.html public/index.html && npm run start",
    "start:tv": "cd public && rm ./index.html && ln -s ../tv.html index.html && cd .. && npm run start",
    "start:index": "cd public && rm ./index.html && ln -s ../index.html index.html && cd .. && REACT_APP_IS_MAIN_DASHBOARD=true npm run start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-fourteen",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts .",
    "clean": "./node_modules/rimraf/bin.js ./lib",
    "remove-unnecessary-dts": "rimraf ./lib/dashboard-react.d.ts",
    "ts-bundle": "npm run clean && tsc --noEmit false && node copy-assets && npm run remove-unnecessary-dts",
    "eject": "react-scripts eject",
    "prepare": "install-peers && npm run ts-bundle"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "Charting library for Netdata Agent",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netdata/dashboard.git"
  },
  "keywords": [
    "netdata",
    "dashboard",
    "charts"
  ],
  "author": "Netdata",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/netdata/dashboard/issues"
